{
  "manifest_version": 2,

  "name": "Braindead",
  "description": "A learning tool to turn a braindead into an expert!",
  "version": "1.0",

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "icons": {
    "16": "braindeadlogo.png",
    "48": "braindeadlogo.png",
    "32": "braindeadlogo.png"
  },

  "browser_action": {
    "default_icon": "braindeadlogo.png"
  },

  "background": {
    "scripts": ["libs/main.js"],
    "persistent": false
  },

  "permissions": [
    "http://*/",
    "https://*/",
    "http://54.149.101.128/*",
    "activeTab",
    "storage",
    "tabs"
  ],

  "web_accessible_resources": [
    "templates/braindead_quiz_tab.html",
    "templates/braindead_bottom.html",
    "templates/braindead_main.html",
    "libs/backbone-min.map",
    "images/braindeadlogo.png"
  ],

  "externally_connectable": {
    "ids": [
      "geeckejfhgpobdkipdbknmbffncpofcm",
      "bbbbbbbbbbbbbbbbbbbbbbbbbbbbbbbb",
      "*"
    ],

    "matches": []
  }

}
